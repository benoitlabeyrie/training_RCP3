<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="com.sogeti.rental.ui.category"
            class="com.sogeti.rental.ui.RentalPropertyView"
            icon="icons/Rentals.png"
            id="com.sogeti.rental.ui.rentalView"
            name="Rental Property View"
            restorable="true">
      </view>
      <category
            id="com.sogeti.rental.ui.category"
            name="Rental">
      </category>
      <view
            category="com.sogeti.rental.ui.category"
            class="com.sogeti.rental.ui.RentalAgencyView"
            icon="icons/Agency.png"
            id="com.sogeti.rental.ui.RentalAgencyView"
            name="Rental Agency View">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.sogeti.rental.ui.perspectives.RentalPerspective"
            icon="icons/Agency.png"
            id="com.sogeti.rental.ui.perspectives.RentalPerspective"
            name="Rental">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.sogeti.rental.ui.prefs.RentalPreferences"
            id="com.sogeti.rental.ui.prefs.rental"
            name="Colors">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.sogeti.rental.ui.prefs.RentalDefaultPreferences">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="com.sogeti.rental.ui.commands.helloworld"
            name="Hello World">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.sogeti.rental.ui.handlers.HelloWorldHandler"
            commandId="com.sogeti.rental.ui.commands.helloworld">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <command
               commandId="com.sogeti.rental.ui.commands.helloworld"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.views.ResourceNavigator?after=additions">
         <command
               commandId="com.sogeti.rental.ui.commands.helloworld"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:com.sogeti.rental.ui.RentalAgencyView?after=additions">
         <command
               commandId="com.sogeti.rental.ui.commands.helloworld"
               style="push">
         </command>
      </menuContribution>
   </extension>

</plugin>
